{
  "name": "qb-lullaby-dashboard",
  "version": "1.0.0",
  "description": "Lullaby dashboard",
  "keywords": [
    "movie-list",
    "movie",
    "item-list",
    "item-browser"
  ],
  "homepage": "",
  "author": "autioch",
  "license": "ISC",
  "main": "index.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "antd": "^4.9.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-virtualized": "^9.22.3",
    "sass": "^1.30.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint ./src --fix --ext .js & exit 0",
    "lint:sass": "sass-lint -c .sass-lint.yml -v -q & exit 0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "qb"
    ],
    "rules": {
      "max-len": [
        0
      ],
      "id-blacklist": [
        "off"
      ],
      "no-empty-function": [
        "off"
      ],
      "no-use-before-define": [
        "error",
        {
          "functions": false,
          "variables": true,
          "classes": true
        }
      ],
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used",
          "ignoreRestSiblings": false
        }
      ],
      "no-undefined": [
        "off"
      ],
      "no-inline-comments": [
        "off"
      ],
      "line-comment-position": [
        "off"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-qb": "^1.0.0"
  }
}
